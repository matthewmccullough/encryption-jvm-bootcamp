<project>
  <modelVersion>4.0.0</modelVersion>
  
  <groupId>com.ambientideas.cryptography</groupId>
  <artifactId>crypto-samples</artifactId>
  <version>1.0.2</version>
  
  <packaging>pom</packaging>
  <name>encryption-samples-masterproject</name>
  <description>A series of examples that demonstrate encryption on the JVM</description>  
  <url>http://github.com/matthewmccullough/encryption-jvm-bootcamp</url>
  
  <developers>
  	<developer>
  		<id>matthew.mccullough</id>
  		<name>Matthew McCullough</name>
  		<email>matthewm@ambientideas.com</email>
  		<url>http://ambientideas.com/blog</url>
  		<organization>Ambient Ideas, LLC</organization>
  		<organizationUrl>http://ambientideas.com</organizationUrl>
  		<timezone>GMT-7</timezone>
  		<properties>
  			<HoursContributed>140</HoursContributed>
  		</properties>
  		<roles>
  			<role>Lead</role>
  		</roles>
  	</developer>
  </developers>
  
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  
  <build>
    <defaultGoal>test</defaultGoal>
    
    <plugins>
      <plugin>  
        <artifactId>maven-compiler-plugin</artifactId>  
        <configuration>  
          <source>1.6</source>  
          <target>1.6</target>  
        </configuration>  
      </plugin>
    </plugins>
  </build>
  
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  
  <modules>    
    <module>base64-encode</module>
    <module>bc-digest</module>
    <module>bc-pgp</module>
    <module>bc-symmetric</module>
    <module>delay-as-brute-force-defense</module>
    <module>homegrown-image-steganography</module>
    <module>homegrown-rsa-simple-algorithm</module>
    <module>homegrown-salt-on-hibernate</module>
    
    <module>jasypt-hash</module>
    <module>jasypt-hibernate</module>
    <module>jasypt-pbe</module>
    <module>jasypt-spring3-example</module>

    <module>jce-asymmetric-rsa-encrypt</module>
    <module>jce-flexiprovider-elliptic-curve</module>
    <module>jce-hmac</module>
    <module>jce-keygen-rsa</module>
    <module>jce-keystore-symmetric-encrypt</module>
    <!-- jce-keytool-keys - RUN A SHELL SCRIPT -->
    <module>jce-message-digest</module>
    <module>jce-rsatoecc-headtoheadtest</module>
    <module>jce-symmetric-encrypt</module>
    
    <!-- <module>jsse-sample-server-sun</module> -->
    <!-- <module>postgresql-encrypt</module> RUN SHELL SCRIPT-->
    <!-- <module>keyczar-examples</module> RUN SHELL SCRIPT-->
    <!-- <module>postgresql-encrypt</module> RUN SHELL SCRIPT-->
    <module>secure-random</module>
    <!-- <module>simple-rot</module> -->
    <!-- <module>squid-filter</module> SHELL SCRIPT or WebDriver-->
  </modules>
</project>